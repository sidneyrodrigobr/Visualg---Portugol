Algoritmo "numeros"


Var
      x, y, z, a, b, c, d: real



inicio
  x <- 2
  y <- 3
  z <- -7

  a <- exp(x, y)     // 2³ = 8
  b <- raizQ(y)       // v3 ˜ 1.73
  c <- pi            // ˜ 3.14159
  d <- abs(z)        // |-7| = 7

  escreval(a)
  escreval(b)
  escreval(c)
  escreval(d)

Fimalgoritmo