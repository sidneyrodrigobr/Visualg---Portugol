Algoritmo "consumo"

Var
    distPerc, combGast, consMedio : real


Inicio
   escreva ("Distancia percorrida KM: ")
   leia (distPerc)
   escreva ("Combustível gasto LT: ")
   leia (combGast)
   
   consMedio <- combGast / distPerc
   
   escreval ("Consumo medio é de : ", consMedio, " KM por litro ")
   
   


Fimalgoritmo